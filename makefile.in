COMPILE=@COMPILE@
all: asreti decdata encdata emreti
asreti: asreti.c makefile
	$(COMPILE) -o $@ $<
decdata: decdata.c makefile
	$(COMPILE) -o $@ $<
encdata: encdata.c makefile
	$(COMPILE) -o $@ $<
emreti: emreti.c makefile
	$(COMPILE) -o $@ $<
format:
	clang-format -i *.c
clean:
	rm -f asreti decdata encdata emreti makefile
	+make -C test clean
test: all
	make -C test
.PHONY: all clean format test
